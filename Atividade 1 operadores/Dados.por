programa {
  funcao inicio() {
    cadeia nome, endereco
    inteiro idade

    escreva("Digite seu nome: ")
    leia(nome)

    escreva("Digite sua idade: ")
    leia(idade)

    escreva("Digite seu endereço: ")
    leia(endereco)

    escreva("Seu nome é " + nome + " \nSua idade é " + idade + "\nSeu endereço é " + endereco)
  }
}
